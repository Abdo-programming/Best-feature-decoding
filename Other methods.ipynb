   "source": [
    "#Logistic regression performed the best. Let's do some hyperparameter tuning\n",
    "scores2 = []\n",
    "for roi in rois:\n",
    "    if roi == 'fixer':\n",
    "        break \n",
    "    roi_df = data_extractor(roi = roi, p = 'bfd/best_features/1000/', key = key, layers = layers, subjs = subjs)\n",
    "    \n",
    "    for layer in layers:\n",
    "            for subj in subjs:\n",
    "                df = roi_df[(roi_df.Subject == subj) & (roi_df.Layer == layer)]\n",
    "                clf = RandomizedSearchCV(\n",
    "                    LogisticRegression(penalty='l1', solver='saga'),\n",
    "                    {'C':[1, 5, 10], 'max_iter':[10, 50, 100, 500]},\n",
    "                    cv = 7,\n",
    "                    n_iter = 4, #1/3 of the parameter space available\n",
    "                    return_train_score = False,\n",
    "                    n_jobs = 4 #To help it run faster while allowing for multitasking on my pc. Change as necessary\n",
    "                )\n",
    "                \n",
    "                clf.fit(df.iloc[:, 3:], df.iloc[:, 0])\n",
    "                scores2.append({\n",
    "                    'ID': str(roi)+\"_\"+str(layer)+\"_\"+str(subj),\n",
    "                    'best_score':clf.best_score_,\n",
    "                    'best_params':clf.best_params_\n",
    "                })"
   ]
  },
